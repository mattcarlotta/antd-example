{
  "name": "react-start-kit",
  "version": "2.0.0",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/react-starter-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "analyze": "better-npm-run analyze",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start": {
      "command": "node server.js",
      "env": {
        "HOST": "http://localhost:",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "rimraf dist && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "analyze": {
      "command": "rimraf dist && webpack",
      "env": {
        "NODE_ENV": "production",
        "NODE_ENV_DEV": "staging",
        "ANALYZE": true
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint **/*.scss",
      "git add"
    ],
    "*.{json,md}": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^4.1.3",
    "better-npm-run": "^0.1.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.4.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "babel-core": "^7.0.0-beta.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.8.2",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^3.3.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-dev-utils": "^10.0.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^4.0.0"
  }
}
