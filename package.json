{
  "name": "react-start-kit",
  "version": "2.0.0",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/react-starter-kit",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10",
    "npm": ">=3"
  },
  "scripts": {
    "analyze": "rimraf .next/analyze && analyze=true npm run build",
    "build": "NODE_OPTIONS=\"-r esm\" NODE_ENV=production next build",
    "checkbuild": "es-check es5 './dist/static/**/*.js'",
    "dev": "NODE_OPTIONS=\"-r esm\" NODE_env=development next dev",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .js,.jsx",
    "lint:styles": "stylelint **/*.scss",
    "start": "NODE_ENV=production next start -p 8080",
    "test": "NODE_ENV=test jest --config ./jest.json --passWithNoTests",
    "test:failed": "NODE_ENV=test jest --config ./jest.json --watch --onlyFailures",
    "test:cov": "NODE_ENV=test jest --config ./jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "NODE_ENV=test jest --config ./jest.json --watchAll",
    "outputs": "yarn analyze && git checkout dynamic && yarn analyze && git checkout unoptimized && yarn analyze && git checkout master"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint"
    ],
    "*.scss": [
      "npm run lint:styles"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nivo/bar": "^0.61.1",
    "@nivo/pie": "^0.61.1",
    "antd": "3.26.6",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "chalk": "^4.0.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.random": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-timezone": "^0.5.28",
    "mongoose": "^5.9.9",
    "mongoose-paginate-v2": "^1.3.9",
    "morgan": "^1.10.0",
    "next": "^9.3.5",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "qs": "^6.9.3",
    "react": "16.8.6",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "16.8.6",
    "react-icons": "^3.9.0",
    "react-quill": "^1.3.5",
    "react-redux": "^7.2.0",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
