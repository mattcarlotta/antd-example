    "browserslist": "^4.11.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "error-overlay-webpack-plugin": "^0.4.1",
    "es-check": "^5.1.0",
    "eslint": "^6.7.2",
